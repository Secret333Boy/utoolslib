{
  "name": "utoolslib",
  "version": "0.0.3",
  "description": "Simple library for text parsing and modificaiton.",
  "main": "./src/index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint . --ignore-path .gitignore && prettier --ignore-path .gitignore -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/*.yml\"",
    "lint:fix": "eslint . --ignore-path .gitignore --fix && prettier --ignore-path .gitignore --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "eslint": "^8.0.1",
    "eslint-config-metarhia": "^7.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Secret333Boy/utoolslib.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "parsing",
    "text",
    "encryption"
  ],
  "contributors": [
    "Eugene Pashkovsky <pashkovskiy.eugen@gmail.com>",
    "Grigoriy Ustymenko <gromgoriy@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Secret333Boy/utoolslib/issues"
  },
  "homepage": "https://github.com/Secret333Boy/utoolslib#readme"
}
